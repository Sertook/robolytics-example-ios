# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Robolytics Example iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Robolytics Example iOS
  pod 'Firebase/Core'
  pod 'Firebase/Analytics'
  pod 'Firebase/RemoteConfig'
  
  pod 'Robolytics', '1.0.0-BETA2'

  target 'Robolytics Example iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Robolytics Example iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
